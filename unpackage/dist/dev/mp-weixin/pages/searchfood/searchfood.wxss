template {
	margin: 0;
	padding:0;
	width: 100%;
	height: 100%;
}
.container>._p:first-child{
	z-index: 1;
	position: absolute;
	top:30%;
	left: calc(50% - 185rpx);
	text-align: center;
}
.container>button{
	position: absolute;
	top:80%;
	left:30%;
	right: 30%;
	text-align: center;
}
.mainbg {
	position: absolute;
	width: 100%;
	height: 100%;
	background-image: url('https://images.nickname4th.vip/img2.jpg');
	background-position: center;
	background-size: cover;
	background-repeat: no-repeat;
	-webkit-filter: blur(5rpx);
	        filter: blur(5rpx);
	opacity: 0.8;
}
.display {
	position: absolute;
	background-size: cover;
	background-repeat: no-repeat;
	text-align: center;
	left: calc(50% - 185rpx);
	top:calc(50% - 185rpx);
	border: 20rpx solid #f8f8f8;
	width: 350rpx;
	height: 350rpx;
	border-radius: 20rpx;
	background-color: #f8f8f0;
	background-image: url('https://images.nickname4th.vip/add.png');
}
.upimg {
	width: 100%;
	height:100%;
	border-radius: 20rpx;
}
.upbutton {
	position: absolute;
	text-align: center;
}

